Algoritmo Ejerc012
	//Indica su un numero es positivo o negativo(si es igual a cero no hace nada)
	Definir n1 Como Entero;
	Escribir "Ingresa un numero :";
	leer n1;
	
	si n1>0 entonces
		escribir "El numero es positivo";
	FinSi
	si n1<0 entonces
		Escribir "El numero es Negativo";
	FinSi
	
FinAlgoritmo
