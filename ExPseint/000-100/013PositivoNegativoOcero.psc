Algoritmo Ejerc013
	//Definir si un numero dado es Positivo, Negativo o igual a cero
	
	Definir n1 Como Real;
	Escribir "Dame un dato: ";
	leer n1;
	si n1>0   Entonces
		escribir "El numero es Positivo";
	sino
		si n1=0 entonces 
			Escribir "El numero es 0";
		SiNo
			escribir "El numero es Negativo";
		FinSi
		
	FinSi
	
FinAlgoritmo
