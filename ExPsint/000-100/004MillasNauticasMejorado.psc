Algoritmo Ejercicio004_MillasNauticas
	Definir  distanciaTotal, distancia,millas Como Real;
	millas=1852;
	distanciaTotal=0;
	Escribir "CONVERSOR DE MILLAS NAUTICAS A METROS : ";
	Escribir "Escriba el valor de la distancia en millas nauticas";
	leer distancia;
	distanciaTotal=distancia*millas;
	Escribir distancia," Millas Nauticas es igual a: ",distanciaTotal," Metros"; 
FinAlgoritmo
