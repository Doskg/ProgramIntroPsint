Algoritmo Ejemplo002
	Definir num Como Entero;
	num <- 0;
	Escribir 'Dime el valor de un numero entero, por favor.';
	Leer num;
	Escribir 'El valor del numero es : ',num;
	Escribir 'Y si lo multiplicamos por 2 es: ', Sin Saltar;
	Escribir num*2;
	Escribir 'Esta frase debe aparecer en una nueva linea';
FinAlgoritmo
